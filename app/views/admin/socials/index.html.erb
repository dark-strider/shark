<h1>Listing socials</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Provider</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @socials.each do |social| %>
  <tr>
    <td><%= social.name %></td>
    <td><%= social.provider %></td>
    <td><%= link_to 'Show', [:admin, social] %></td>
    <td><%= link_to 'Edit', [:edit, :admin, social] %></td>
    <td><%= link_to 'Destroy', [:admin, social], confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New social', [:new, :admin, :social] %>
